<!DOCTYPE html>
<!-- saved from url=(0037)#home -->
<html lang="en">

<head>
    {{> head}}

<body>
    <div class="app-container app-theme-white body-tabs-shadow fixed-header fixed-sidebar">
        {{> header}}
        <div class="app-main">
            {{> sidebar active_news_management_2=true}}
            <div class="app-main__outer">
                <div class="app-main__inner">
                    <div class="app-page-title">
                        <div class="page-title-wrapper">
                            <div class="page-title-heading">
                                <div class="page-title-icon">
                                    <i class="pe-7s-wallet icon_gradient bg-tempting-azure"> </i>
                                </div>
                                <div>News Management
                                    <div class="page-title-subheading">List Berita
                                    </div>
                                </div>
                            </div>
                            <div class="page-title-actions">
                                <div class="d-inline-block dropdown">
                                    <a href="./grosir_product_tambah.html" class="btn btn-info tooltips btn-shadow"
                                        data-toggle="tooltip" title="" data-original-title="Tambah Produk"><i
                                            class="icon fa fa-plus" aria-hidden="true"></i> Create News</a>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="main-card mb-3 card">
                        <div class="card-body">
                            <div id="table_wrapper" class="dataTables_wrapper dt-bootstrap4 no-footer">
                                <div class="row">
                                    <div class="col-sm-12 col-md-6">
                                        <div class="dataTables_length d-inline-block mr-4" id="table_length">
                                            <div class="dropdown w-100">
                                                <button class="btn btn-secondary btn-sm dropdown-toggle" type="button"
                                                    id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true"
                                                    aria-expanded="false">
                                                    <i class="fas fa-filter mr-2"></i> <span
                                                        class="d-none d-md-inline-block">Filter</span>
                                                </button>
                                                <div class="dropdown-menu dropdown-width" aria-labelledby="dropdownMenuButton">
                                                    <div class="p-3">
                                                        <form enctype="multipart/form-data"
                                                            action="https://dashboard.simpeldesa.com/grosir-produk/add/post"
                                                            id="form" role="form" method="post">
                                                            <input type="hidden" name="id" class="input-xlarge"
                                                                placeholder="" value="">
                                                            <div class="main-card mb-3">
                                                                <div class="form-group row">
                                                                    <div class="col-sm-12 col-md-3">
                                                                        <label class="control-label">User Role</label>
                                                                    </div>
                                                                    <div class="col-sm-12 col-md-9">
                                                                        <select class="form-control" id="exampleFormControlSelect1">
                                                                            <option>1</option>
                                                                            <option>2</option>
                                                                            <option>3</option>
                                                                            <option>4</option>
                                                                            <option>5</option>
                                                                        </select>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group row">
                                                                    <div class="col-sm-12 col-md-3">
                                                                        <label class="control-label">Kategori</label>
                                                                    </div>
                                                                    <div class="col-sm-12 col-md-9">
                                                                        <span
                                                                            class="select2 select2-container select2-container--default"
                                                                            dir="ltr" data-select2-id="2" style="width:100%">
                                                                            <span class="selection"><span
                                                                                    class="select2-selection select2-selection--single"
                                                                                    role="combobox" aria-haspopup="true"
                                                                                    aria-expanded="false" tabindex="0"
                                                                                    aria-labelledby="select2-category_id-container"><span
                                                                                        class="select2-selection__rendered"
                                                                                        id="select2-category_id-container"
                                                                                        role="textbox"
                                                                                        aria-readonly="true"
                                                                                        title="Ayam &amp; Daging">Bogor</span><span
                                                                                        class="select2-selection__arrow"
                                                                                        role="presentation"><b
                                                                                            role="presentation"></b></span></span></span><span
                                                                                class="dropdown-wrapper"
                                                                                aria-hidden="true"></span></span>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group row">
                                                                    <div class="col-sm-12 col-md-3">
                                                                        <label class="control-label">Medium</label>

                                                                    </div>
                                                                    <div class="col-sm-12 col-md-9">
                                                                        <select class="form-control" id="exampleFormControlSelect1">
                                                                            <option>Website</option>
                                                                            <option>Mobile Apps</option>
                                                                        </select>

                                                                    </div>
                                                                </div>
                                                                <div class="form-group row">
                                                                    <div class="col-sm-12 col-md-3">
                                                                        <label class="control-label">Date
                                                                            Created</label>
                                                                    </div>
                                                                    <div class="col-sm-12 col-md-9 row">
                                                                        <div class="col-5">
                                                                            <div class="input-group">
                                                                                <input type="date" id="startDate"
                                                                                    class="form-control"
                                                                                    aria-label="Start Date">
                                                                                <div class="input-group-append">
                                                                                    <span class="input-group-text"><i
                                                                                            class="icon ion-android-calendar"></i></span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="col-2 text-center">
                                                                            <h4 class="mb-0">-</h4>
                                                                        </div>
                                                                        <div class="col-5">
                                                                            <div class="input-group">
                                                                                <input type="date" id="startDate"
                                                                                    class="form-control"
                                                                                    aria-label="Start Date">
                                                                                <div class="input-group-append">
                                                                                    <span class="input-group-text"><i
                                                                                            class="icon ion-android-calendar"></i></span>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group row">
                                                                    <div class="col-sm-12 col-md-3">
                                                                        <label class="control-label">Status</label>

                                                                    </div>
                                                                    <div class="col-sm-12 col-md-9">
                                                                        <select class="form-control" id="exampleFormControlSelect1">
                                                                            <option>Published</option>
                                                                            <option>Unpublished</option>
                                                                        </select>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </form>
                                                    </div>
                                                    <div class="dropdown-divider"></div>
                                                    <div class="dropdown-item align-right">
                                                        <button type="button" class="btn btn-primary mr-2">TERAPKAN</button>
                                                        <button type="button" class="btn btn-danger">BATALKAN</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="dataTables_length d-inline-block" id="table_length">
                                            <select name="table_length" aria-controls="table"
                                                class="custom-select custom-select-sm form-control form-control-sm">
                                                <option value="10">10</option>
                                                <option value="25">25</option>
                                                <option value="50">50</option>
                                                <option value="100">100</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-sm-12 col-md-6">
                                        <div id="table_filter" class="dataTables_filter">
                                            <label>Search:
                                                <input type="search" class="form-control form-control-sm" placeholder=""
                                                    aria-controls="table"></label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12">
                                        <table
                                            class="table table-striped table-bordered table-hover table-full-width dataTable no-footer"
                                            id="table" role="grid" aria-describedby="table_info">
                                            <thead>
                                                <tr role="row">
                                                    <th class="sorting" tabindex="0" aria-controls="table" rowspan="1"
                                                        colspan="1"
                                                        aria-label="Nama Produk: activate to sort column ascending">
                                                        Dibuat</th>
                                                    <th class="sorting" tabindex="0" aria-controls="table" rowspan="1"
                                                        colspan="1"
                                                        aria-label="Harga: activate to sort column ascending">User Role
                                                    </th>
                                                    <th class="sorting" tabindex="0" aria-controls="table" rowspan="1"
                                                        colspan="1"
                                                        aria-label="Harga: activate to sort column ascending">Kategori
                                                    </th>
                                                    <th class="sorting" tabindex="0" aria-controls="table" rowspan="1"
                                                        colspan="1"
                                                        aria-label="Stock: activate to sort column ascending">Judul</th>
                                                    <th class="sorting" tabindex="0" aria-controls="table" rowspan="1"
                                                        colspan="1"
                                                        aria-label="Kategori: activate to sort column ascending">Medium
                                                    </th>
                                                    <th class="sorting" tabindex="0" aria-controls="table" rowspan="1"
                                                        colspan="1"
                                                        aria-label="Status: activate to sort column ascending">Created
                                                        Date</th>
                                                    <th class="align-center sorting_disabled" rowspan="1" colspan="1"
                                                        aria-label="Aksi">Aksi
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody>

                                                <tr role="row" class="odd">
                                                    <td>Marini Zumarnis</td>
                                                    <td>Kepala Desa</td>
                                                    <td>Berita Desa</td>
                                                    <td>Kegiatan Desa Cisayong saat ramadhan</td>
                                                    <td>Web</td>
                                                    <td>12-03-2020</td>
                                                    <td class=" align-center">
                                                        <button class="mb-2 mr-2 btn btn-sm btn-shadow-primary btn-secondary"
                                                            title="Ubah" data-toggle="modal"
                                                            data-target="#modal-published"><i
                                                                class="ion-android-arrow-down mr-2"></i> Unpublished
                                                        </button>
                                                    </td>
                                                </tr>
                                                <tr role="row" class="odd">
                                                    <td>Marini Zumarnis</td>
                                                    <td>Kepala Desa</td>
                                                    <td>Berita Desa</td>
                                                    <td>Kegiatan Desa Cisayong saat ramadhan</td>
                                                    <td>Web</td>
                                                    <td>12-03-2020</td>
                                                    <td class="align-center">
                                                        <button
                                                            class="mb-2 mr-2 btn btn-sm btn-shadow-primary btn-success"
                                                            title="Ubah" data-toggle="modal"
                                                            data-target="#modal-published"><i
                                                                class="ion-android-arrow-up mr-2"></i> Publish
                                                        </button>
                                                    </td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                {{> footer}}
            </div>
        </div>
    </div>

    {{> modal_published}}

    {{> modal_form}}

    {{> modal_komentar}}

    {{> js}}
    <link rel="stylesheet" href="https://dashboard.simpeldesa.com/assets/js/vendors/form-components/select2.min.css" />

    <script type="text/javascript"
        src="https://dashboard.simpeldesa.com/assets/js/vendors/form-components/select2.full.min.js"></script>

    <script src="https://dashboard.simpeldesa.com/assets/js/vendors/jquery.sticky-sidebar.min.js"></script>
    <script src="https://dashboard.simpeldesa.com/assets/js/vendors/tinymce/js/tinymce/tinymce.min.js"></script>
    <link href="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.5.0/css/bootstrap4-toggle.min.css"
        rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.5.0/js/bootstrap4-toggle.min.js"></script>
    <script type="text/javascript">
        $('.select2').select2();
        $('.select3').select3();
    </script>
</body>

</html>